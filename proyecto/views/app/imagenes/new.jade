extends ../../layout.jade
block contenido
    div(class="col-md-6 remove-float center-block big-top-space")
        h1 Subir nueva imagen
        form(action="/app/imagenes",method="POST")
            div(class="form-group")
                label(for="title") Titulo
                input(type="text", name="title", placeholder="Email", id="title",class="form-control")
            div
                label(for="archivo") Archivo
                input(type="file", name="password", placeholder="Password", id="archivo",class="form-control")
            div(class="row top-space")
                div(class="col-xs-12 col-sm-6")
                    input(type="submit" value="Subir imagen" class="btn btn-info")
                div(class="col-xs-12 col-sm-6 text-right")
                    a(href="/signup") No tengo cuenta